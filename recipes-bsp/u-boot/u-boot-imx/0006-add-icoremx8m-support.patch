 board/engicam/icoremx8m/lpddr4_timing.c    | 1734 ++++++++++++++++++++
 board/engicam/icoremx8m/lpddr4_timing_b0.c | 1734 ++++++++++++++++++++
 board/engicam/icoremx8m/spl.c              |  253 +++
 configs/icoremx8m_defconfig                |   74 +
 include/configs/icoremx8m.h                |  299 ++++
 12 files changed, 4941 insertions(+), 2 deletions(-)
 create mode 100644 arch/arm/dts/icoremx8m.dts
 create mode 100644 board/engicam/common/Kconfig
 create mode 100644 board/engicam/icoremx8m/Kconfig
 create mode 100644 board/engicam/icoremx8m/Makefile
 create mode 100644 board/engicam/icoremx8m/icoremx8m.c
 create mode 100755 board/engicam/icoremx8m/lpddr4_timing.c
