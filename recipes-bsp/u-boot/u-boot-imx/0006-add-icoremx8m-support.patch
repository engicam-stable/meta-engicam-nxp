diff --git a/arch/arm/dts/Makefile b/arch/arm/dts/Makefile
index cce56c6..46a411b 100644
--- a/arch/arm/dts/Makefile
+++ b/arch/arm/dts/Makefile
@@ -469,7 +469,8 @@ dtb-$(CONFIG_ARCH_IMX8M) += fsl-imx8mq-evk.dtb \
 		fsl-imx8mm-ddr4-evk.dtb \
 		fsl-imx8mm-ddr4-val.dtb \
 		fsl-imx8mm-evk.dtb \
-		icore-imx8mm.dtb
+		icore-imx8mm.dtb \
+		icoremx8m.dtb

 dtb-$(CONFIG_ARCH_IMX8) += fsl-imx8dx-ddr3-arm2.dtb \
 		fsl-imx8qm-ddr4-arm2.dtb \
diff --git a/arch/arm/dts/icoremx8m.dts b/arch/arm/dts/icoremx8m.dts
new file mode 100644
index 0000000..950aa9d
--- /dev/null
+++ b/arch/arm/dts/icoremx8m.dts
@@ -0,0 +1,415 @@
+/*
